{
  "repoLink": "https://github.com/evan361425/flutter-arb-glue",
  "changelog": {
    "template": "{content}",
    "commitMessage": "chore: bump to {version}"
  },
  "latestInfo": {
    "diff": {
      "enable": true,
      "allowed": ["feat:", "fix:"]
    }
  },
  "tags": {
    "release": {
      "pattern": "v[0-9]+.[0-9]+.[0-9]+",
      "changelog": true,
      "release": {
        "enable": true
      }
    }
  }
}
